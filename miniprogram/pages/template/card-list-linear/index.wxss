.card-list-linear.card-list-box {
  padding: 20rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.card-list-linear .card-list-li{
  display: flex;
  
}
.card-list-linear .card-list-img{
  width: 200rpx;
  height: 200rpx;
}
.card-list-linear .card-list-img image{
  border-radius: 10rpx;
}


.card-list-linear .card-list-msg{
  margin-top: 10rpx;
  flex: 1;
  line-height: 44rpx;
}

